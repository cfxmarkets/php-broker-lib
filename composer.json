{
    "name": "cfxmarkets/php-brokerage-sdk",
    "description": "A public PHP api library to access the CFX Markets brokerage api",
    "type": "library",
    "keywords": [ "cfx", "api", "client", "brokerage" ],
    "license": "MIT",
    "authors": [
        {
            "name": "CFX and contributors",
            "homepage": "https://github.com/cfxmarkets/php-broker-lib/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require" : {
        "php" : ">=5.4.0",
        "psr/http-message": "~1.0",
        "cfxmarkets/php-persistence": "~0.1.0",
        "cfxmarkets/php-public-models": "~1.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.8.0",
        "guzzlehttp/guzzle" : "~5.0"
    },
    "autoload" : {
        "psr-4" : {
            "CFX\\SDK\\Brokerage\\" : "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../php-persistence"
        },
        {
            "type": "path",
            "url": "../php-public-models"
        },
        {
            "type": "path",
            "url": "../php-jsonapi-objects"
        },
        {
            "type": "git",
            "url": "https://github.com/cfxmarkets/php-std-traits.git"
        },
        {
            "type": "git",
            "url": "https://github.com/cfxmarkets/php-jsonapi-objects.git"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-v1.0.x": "v1.0.x-dev"
        }
    }
}
